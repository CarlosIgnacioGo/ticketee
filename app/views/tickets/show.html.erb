<div id='ticket'>
  <h2><%= @ticket.title %></h2>
  <%= simple_format(@ticket.description) %>
  <%= link_to 'Editar', edit_project_ticket_path(@project, @ticket) %>
  <%= link_to "Delete ticket", [@project, @ticket], method: :delete, data: { confirm:"Are you sure you want to delete this project?"} %>
</div>



